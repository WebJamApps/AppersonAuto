  "scripts": {
    "start:dist": "node server.js",
    "build:prod": "rm -rf dist && npm start build",
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "start": "nps",
    "test:lint-fix": "npm run test:stylelint && nps test.lint.fix",
    "test:lint": "npm run test:stylelint && nps test.lint",
    "test": "npm run test:lint && npm run test:puppeteer && npm run test:jest",
    "test:local": "npm run test:lint-fix && npm run test:jest && npm run test:puppeteer && npm run cc",
    "test:jest": "jest --config jest.config.json --no-cache --runInBand",
    "test:puppeteer": "jest --preset jest-puppeteer --config jest.puppeteer-config.json /test/puppeteer --runInBand",
    "cleaninstall": "rm -rf yarn.lock && rm -rf package-lock.json && rm -rf node_modules && npm install",
    "postinstall": "npm run build:prod",
    "test:stylelint": "stylelint \"static/**/*.scss\""
  },
